# Ambiente
NODE_ENV=development

# URL base da aplicação
NEXT_PUBLIC_APP_URL=http://localhost:3000
# Em produção: https://dash.agentesdeconversao.com.br

# Credenciais de autenticação (usando NextAuth)
NEXTAUTH_URL=http://localhost:3000
# Em produção: https://dash.agentesdeconversao.com.br
NEXTAUTH_SECRET=your-nextauth-secret-key

# Banco de dados
DATABASE_URL=postgresql://user:password@localhost:5432/agentes-conversao

# URLs da API
NEXT_PUBLIC_API_URL=http://localhost:3000/api
# Em produção: https://dash.agentesdeconversao.com.br/api

# Integrações
OPENAI_API_KEY=your-openai-api-key

# Analytics
NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id